<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
 <dict>
   <key>Label</key>
   <string>com.user.rclone-org</string>

   <key>ProgramArguments</key>
   <array>
     <string>/usr/local/bin/rclone</string>
     <string>mount</string>
     <string>tabdigital:org</string>
     <string>/Users/haifeng/org</string>
     <string>--vfs-cache-mode</string>
     <string>writes</string>
   </array>

   <key>RunAtLoad</key>
   <true/>

   <key>KeepAlive</key>
   <true/>

   <key>StandardOutPath</key>
   <string>/Users/haifeng/rclone-mount.log</string>

   <key>StandardErrorPath</key>
   <string>/Users/haifeng/rclone-mount.log</string>
 </dict>
</plist>
